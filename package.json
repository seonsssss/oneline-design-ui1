{
    "name": "@oneline-design-ui",
    "private": true,
    "description": "",
    "scripts": {
        "build": "turbo build",
        "prelint": "pnpm run build",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "lint:fix": "pnpm run lint --fix",
        "prettier": "prettier --check '**/*.{ts,tsx,js,jsx,json}'",
        "prettier:fix": "prettier --write '**/*.{ts,tsx,js,jsx,json}'",
        "start:docs": "turbo start:docs --filter ui-docs",
        "build:docs": "turbo build:docs",
        "build:watch": "turbo build:watch"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.4",
        "@oneline-design-ui/eslint-config": "workspace:^",
        "@oneline-design-ui/prettier-config": "workspace:^",
        "builtin-modules": "^4.0.0",
        "prettier": "^3.2.5",
        "turbo": "^2.3.3",
        "typescript": "^5.2.2"
    },
    "packageManager": "pnpm@10.10.0",
    "engines": {
        "node": ">=20",
        "pnpm": ">=9"
    }
}
